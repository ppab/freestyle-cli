{
  "create_frontend_modules": [
    {
      "name": "pipe",
      "args": {
        "commands": [
          {
            "name": "createFileFromEntriesCommand",
            "args": {
              "entities": {
                "filePath": "./freestyle/entities.json"
              },
              "frontEndModule": {
                "contentDestination": {
                  "path": "./freestyle/frontend-schemas/modules/{{PASCAL_CASE_ENTITY}}/index.ts"
                },
                "templateSource": {
                  "path": "./src/templates/frontend-schemas/simple-schema.txt"
                }
              }
            }
          }
        ]
      }
    }
  ]
}
